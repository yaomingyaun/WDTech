<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/dp_30">
    <TextView
        android:id="@+id/title_search"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="111111111"
        android:textSize="@dimen/sp_14"
        android:textColor="#666666"
        />
    <TextView
        android:id="@+id/name_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title_search"
        android:text="33333333"
        android:layout_marginTop="@dimen/dp_4"
        android:textSize="@dimen/sp_10"
        android:textColor="#999999"
        />
    <TextView
        android:id="@+id/time_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="222222"
        android:textSize="@dimen/sp_10"
        android:textColor="#999999"
        android:layout_marginLeft="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_below="@+id/title_search"
        android:layout_toRightOf="@+id/name_search"
        ></TextView>
    <TextView
        android:id="@+id/minute_search"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="44444"
        android:textSize="@dimen/sp_10"
        android:textColor="#999999"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_marginLeft="@dimen/dp_10"
        android:layout_below="@+id/title_search"
        android:layout_toRightOf="@+id/time_search"
        ></TextView>
</RelativeLayout>
